local player = game:GetService("Players").LocalPlayer
local CollectionService = game:GetService("CollectionService")
local vim = game:GetService("VirtualInputManager")

local G2L = {}

G2L["ScreenGui_1"] = Instance.new("ScreenGui", game:GetService("CoreGui"))
G2L["ScreenGui_1"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling
G2L["ScreenGui_1"].Name = "AutoClickerGUI"
CollectionService:AddTag(G2L["ScreenGui_1"], "main")

G2L["Frame4_2"] = Instance.new("Frame", G2L["ScreenGui_1"])
G2L["Frame4_2"].ZIndex = 1999999
G2L["Frame4_2"].BorderSizePixel = 0
G2L["Frame4_2"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
G2L["Frame4_2"].Size = UDim2.new(0, 52, 0, 52)
G2L["Frame4_2"].Position = UDim2.new(1, -373, 1, -233)
G2L["Frame4_2"].Name = "Frame4"
G2L["Frame4_2"].BackgroundTransparency = 1

local corner1 = Instance.new("UICorner", G2L["Frame4_2"])
corner1.CornerRadius = UDim.new(0, 800)

local stroke = Instance.new("UIStroke", G2L["Frame4_2"])
stroke.Thickness = 5
stroke.Color = Color3.fromRGB(66, 130, 255)

local inner = Instance.new("Frame", G2L["Frame4_2"])
inner.BorderSizePixel = 0
inner.BackgroundColor3 = Color3.fromRGB(115, 167, 255)
inner.Size = UDim2.new(0, 52, 0, 52)
inner.BackgroundTransparency = 0.8

local corner2 = Instance.new("UICorner", inner)
corner2.CornerRadius = UDim.new(0, 500)

local sr1 = Instance.new("Frame", G2L["Frame4_2"])
